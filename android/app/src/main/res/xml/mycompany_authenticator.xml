<!--
Override the authenticator setting under forgerock-auth
If the account is created or created before, the label and icon cannot be updated, change the accountType
to recreate the Account
-->

<account-authenticator
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:accountType="com.mycompany"
    android:icon="@mipmap/ic_launcher"
    android:smallIcon="@mipmap/ic_launcher"
    android:label="@string/forgerock"/>